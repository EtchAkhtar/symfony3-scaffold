<form class="form-horizontal" id="form-demo" action="{{ url('AppBundle_demo') }}" method="post">

	<div class="form-group">
		{{ form_label(form.name) }}
		<div class="col-sm-3">{{ form_widget(form.name, { 'attr': {'class': 'form-control'} }) }}</div>
	</div>
	<div class="form-group">
		{{ form_label(form.age) }}
		<div class="col-sm-3">{{ form_widget(form.age, { 'attr': {'class': 'form-control'} }) }}</div>
	</div>

	{{ form_rest(form) }}

	<div class="col-sm-3"></div>
	<button class="btn btn-primary" type="submit">Save</button>

	{{ form_rest(form) }}

</form>
